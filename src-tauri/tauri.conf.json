{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri-book-box-test",
  "version": "1.0.0",
  "identifier": "com.yoshomay.tauri-book-box",
  "build": {
    "frontendDist": "../src" 
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "tauri-book-box-test",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["deb", "appimage", "rpm", "msi", "nsis"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ] 
  }
}
